<nav class="navbar navbar-expand-sm navbar-dark bg-light mb-3">
  <div class="container">
    <ul class="navbar-nav">
      <p><h2>Temperature</h2></p>
    </ul>
  </div>
</nav>
<div class="container">
  <div class="row bg-light">
    <div class="col-sm">
      <div class="card card-body mb-2 col-sm">
        <h4 align="center">Temperature Value</h4>
        <div class="card card-body mb-2">
          <h6 align="left">Recent value</h6>
          <div id="latest-temp"></div>
          <hr/>
        </div>
        <div class="card card-body mb-2">
          <h6 align="left">Values received previously</h6>
          <div id="lasthour-temp">
            {{#each values}}
              {{#eq this.deviceId "device1"}}
                <c id="{{this.date}}"><font size=2><{{this.value}}></font></c>
              {{/eq}}
              {{#eq this.deviceId "riot_device1"}}
                <c id="{{this.date}}"><font size=2><{{this.value}}></font></c>
              {{/eq}}
            {{/each}}
          </div>
          <hr/>
        </div>
      </div>
    </div>
  </div>
</div>
